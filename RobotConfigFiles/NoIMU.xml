<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot>
    <MotorController name="drive_controller" serialNumber="AL00VBVM">
        <Motor name="left_drive" port="1" />
        <Motor name="right_drive" port="2" />
    </MotorController>
    <MotorController name="collector_controller" serialNumber="AL00UYQ6">
        <Motor name="block_collector" port="1" />
        <Motor name="hanging_motor" port="2" />
    </MotorController>
    <MotorController name="brace_controller" serialNumber="AL00VUP7">
        <Motor name="back_wheel" port="1" />
        <Motor name="back_brace" port="2" />
    </MotorController>
    <ServoController name="second_servo_controller" serialNumber="AI02QU16">
        <Servo name="hang_stop" port="4" />
        <Servo name="left_ramp" port="5" />
        <Servo name="all_clear" port="6" />
    </ServoController>
    <LegacyModuleController name="legacy_module" serialNumber="AL00XTPF">
        <ColorSensor name="color_sensor" port="3" />
    </LegacyModuleController>
</Robot>
